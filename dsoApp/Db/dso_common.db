# P Prefix
# R Record root
# L Link (port)

record(bo, "$(P)$(R):setup")
{
    field(DTYP, "stream")
    field(OUT,  "@dso.proto setup $(L)")
}

record(ai, "$(P)$(R):xincr")
{
    field(DTYP, "stream")
    field(INP,  "@dso.proto xincr $(L)")
    field(SCAN, "5 second")
    field(FLNK, "$(P)$(R):computex")
}

record(aSub, "$(P)$(R):computex")
{
    field(SNAM, "asub_compute_x")
    field(INPA, "$(P)$(R):xincr")
    field(INPB, "1000")
    field(FTA,  "DOUBLE")
    field(FTB,  "DOUBLE")
    field(FTVA, "DOUBLE")
    field(NOVA, "10000")
    field(FLNK, "$(P)$(R):x")
}

record(waveform, "$(P)$(R):x")
{
    field(INP,  "$(P)$(R):computex.VALA")
    field(NELM, "10000")
    field(FTVL, "DOUBLE")
    field(EGU,  "ms")
    field(PREC, "6")
}
